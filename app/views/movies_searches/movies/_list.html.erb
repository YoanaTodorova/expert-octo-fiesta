<ul>
<% movies.find_each do |movie| %>
    <li>
      <% if movie.poster && movie.poster.attached? %>
        <%= image_tag url_for(movie.poster), style: 'max-height: 100px; max-width: 100px;' %>
      <% end %>
      <%= movie.title %>
    </li>
<% end %>
</ul>

<%= paginate movies, params: { controller: 'movies_searches/movies', action: 'index', movies_search_id: search.id }, remote: true %>
